    "connect-session-sequelize": "^6.0.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "morgan": "^1.9.1",
    "npm-merge-driver": "^2.3.5",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
